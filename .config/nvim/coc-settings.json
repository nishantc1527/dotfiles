{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "graphql",
    "html",
    "yaml",
    "json",
    "python",
    "java",
    "rust"
  ],
  // explorer
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
    "<cr>": [
      "expandable?",
      "expand",
      "open"
    ],
    "v": "open:vsplit"
  },
  "cSpell.userWords": [
    "Arial",
    "Chatterjee",
    "Nishant",
    "curr",
    "gruvbox",
    "jfinder",
    "nums",
    "pylint"
  ]
}
