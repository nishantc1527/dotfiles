#include <vector>
#include <map>

#include "../Nodes.h"

class Solution {
public:
  TreeNode* buildTree(std::vector<int>& inorder, std::vector<int>& postorder) {
    std::map<int, int> ind;
    for (int i = 0; i < inorder.size(); i++) {
      ind[inorder[i]] = i;
    }

    TreeNode* root = new TreeNode(postorder.back());
    if (inorder.size() == 1) return root;
    int i = ind[postorder.back()];

    

    return root;
  }
};
